{
   "author": "David G. Daniel",
   "background": {
      "page": "pages/background.html"
   },
   "browser_action": {
      "default_icon": "icons/64x64.png",
      "default_popup": "pages/popup.html",
      "default_title": "Citrus"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/content.js" ],
      "matches": [ "https://dashboard.citrus.app/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Vitamin C for your Productivity",
   "devtools_page": "pages/devtools.html",
   "icons": {
      "1024": "icons/1024x1024.png",
      "128": "icons/128x128.png",
      "16": "icons/16x16.png",
      "24": "icons/24x24.png",
      "256": "icons/256x256.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "512": "icons/512x512.png",
      "64": "icons/64x64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Citrus: Get More Done",
   "permissions": [ "\u003Call_urls>", "*://*/*", "activeTab", "alarms", "tabs", "cookies", "contextMenus", "unlimitedStorage", "storage", "notifications", "webNavigation" ],
   "short_name": "Citrus",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3",
   "web_accessible_resources": [ "panel.html", "js/content.js" ]
}
